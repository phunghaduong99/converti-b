	<sqlMap resource="config/ibatis/oracle/sysadmin/User_role.xml"/>
	<!--角色参数(t_role_param)-->
	<sqlMap resource="config/ibatis/oracle/sysadmin/Role_param.xml"/>
	<!--操作日志表(t_operatelog)-->
	<sqlMap resource="config/ibatis/oracle/sysadmin/Operatelog.xml"/>
	
</sqlMapConfig>
