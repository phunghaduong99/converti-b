			</if>
			<if test="lrsjf != null and lrsjf != ''">and
				a.lrsj &gt;=#lrsjf:DATE#
			</if>
			<if test="lrsjt != null and lrsjt != ''">and
				a.lrsj &lt; #lrsjt:DATE#+1
			</if>
			
			<if test="cyzj != null and cyzj != ''">and
				a.cyzj =#{cyzj}
			</if>
			<if test="cyzjdm != null and cyzjdm != ''">and
				a.cyzjdm =#{cyzjdm}
			</if>
			<if test="zjhm != null and zjhm != ''">and
